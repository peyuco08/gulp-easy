#header .icon-consentimiento {
  background-position: -15px 0;
}

#notificaciones .icon-informativo,
#header .icon-informativo {
  background-position: -30px 0;
}

#header .icon-alerta,
#header .icon-consentimiento,
#header .icon-informativo,
#notificaciones .icon-alerta,
