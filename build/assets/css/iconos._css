
/* ==========================================================================
   ICONS
   ========================================================================== */
[class^="icon-"],
[class*=" icon-"] {
  display: inline-block;
  width: 15px;
  height: 15px;
  line-height: 15px;
  vertical-align: text-top;
  background-position: 15px 15px;
  background-repeat: no-repeat;
  margin-top: 1px;
}  

.icon-mini        { width: 12px; height: 12px; background-position: 12px 12px;  background-image: url(../img/icon-mini-azul.png); }
.icon-gris        { background-image: url(../img/icon-gris.png); }
.icon-blanco      { background-image: url(../img/icon-blanco.png); }
.icon-naranjo     { background-image: url(../img/icon-naranjo.png); }
.icon-grande      { width:20px; height:20px; background-position: 20px 20px; }
.icon-xlarge      { width:64px; height:64px; background-position: 64px 64px; }



[class*="icon-grande"].icon-naranjo { background-image: url(../img/icon-naranjo-large.png); }
[class*="icon-grande"].icon-gris    { background-image: url(../img/icon-gris-large.png); }
[class*="icon-grande"].icon-blanco  { background-image: url(../img/icon-blanco-large.png); }

[class*="icon-xlarge"].icon-naranjo { background-image: url(../img/icon-naranjo-xlarge.png); }
[class*="icon-xlarge"].icon-gris    { background-image: url(../img/icon-gris-xlarge.png); }
[class*="icon-xlarge"].icon-blanco  { background-image: url(../img/icon-blanco-xlarge.png); }


/* ==========================================================================
   ICON HOVER
   ========================================================================== */
.hover-icon:hover [class*="icon-gris"] { background-image: url(../img/icon-azul-p.png); }
.hover-icon:hover [class*="icon-blanco"] { background-image: url(../img/icon-naranjo.png); }


